{
    "Daemon": {
        "DaemonName": "Services.Wrapper.HomeAssistant"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Debug"
        }
    },
    "AllowedHosts": "*",
    "RabbitMq": {

        "Port": 5672,
        "Hostname": "localhost"
    },
    "Mqtt": {
        "Hostname": "localhost",
        "Port": 1883
    },
    "Seq": {
        "ServerUrl": "http://192.168.99.100:5341",
        "MinimumLevel": "Trace",
        "LevelOverride": {
            "Microsoft": "Warning"
        }
    }
}

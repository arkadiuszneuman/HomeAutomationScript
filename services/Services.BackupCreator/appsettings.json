{
    "Daemon": {
        "DaemonName": "Services.BackupCreator"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Debug"
        }
    },
    "Seq": {
        "ServerUrl": "http://192.168.99.100:5341",
        "MinimumLevel": "Trace",
        "LevelOverride": {
            "Microsoft": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Paths": {
        "TempBackupPath": "./../../../../../backup",
        "DirsToBackup": [
            "./../../../../../homeassistant",
            "./../../../../../grafana",
            "./../../../../../influxdb"
        ]
    },
    "Dropbox": {
        "MaxFiles": 7,
        "TimeoutSeconds": 300
    },
    "Schedulers": {
        "BackupHourOfADay": 3,
        "BackupMinuteOfADay": 0
    }
}

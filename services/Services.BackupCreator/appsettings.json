{
    "Daemon": {
        "DaemonName": "Services.BackupCreator"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Debug"
        }
    },
    "ElasticConfiguration": {
        "Uri": "http://localhost:9200/",
        "IndexFormat": "logstash_bc-{0:yyyy.MM.dd}"
    },
    "AllowedHosts": "*",
    "Paths": {
        "TempBackupPath": "./../../../../../backup",
        "DirsToBackup": [
            "./../../../../../homeassistant",
            "./../../../../../grafana",
            "./../../../../../influxdb",
            "./../../../../../elasticsearchdata",
            "./../../../../../kibana",
            "./../../../../../logstash"
        ]
    },
    "Dropbox": {
        "MaxFiles": 7,
        "TimeoutSeconds": 300
    },
    "Schedulers": {
        "BackupHourOfADay": 3,
        "BackupMinuteOfADay": 0
    }
}
